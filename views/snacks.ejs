<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabulka</title>
    <link rel="stylesheet" href="panel.css">
    <script src="panel.js"></script>
</head>

<body background="https://wallpapers.com/images/hd/food-4k-m37wpodzrcbv5gvw.jpg">
    <h1>MENU</h1>
    <table>
        <thead>
            <tr id="nav">
                <th>
                    <div id="navSnacks" class="navItem"
                        style="background: linear-gradient(90deg, rgba(255, 186, 9, 1) 32%, rgba(238, 238, 24, 1) 100%)">
                        <h6>Snacks</h6>
                        <img src="https://images.squarespace-cdn.com/content/v1/54f681f1e4b094fe88e5e857/1442519748040-XPIMZGHJZ6P7WCGAD5O4/snacks.png?format=2500w"
                            class="img" alt="Snacks nav img">
                    </div>
                </th>
                <th <div id="navDeserts" class="navItem"
                    style="background: radial-gradient(circle, rgb(255, 230, 191) 0%, rgb(255, 149, 199) 100%);">
                    <h6>Dezerty</h6>
                    <img src="https://purepng.com/public/uploads/large/purepng.com-cakefood-chocolate-slice-cake-sweet-tasty-piece-eat-delicious-9415246166160hbsv.png"
                        alt="dezerty image" class="img">
                    </div>
                </th>
                <th>
                    <div id="navDrinks" class="navItem"
                        style="background: linear-gradient(22deg, rgba(118, 176, 255, 1) 0%, rgba(130, 255, 169, 1) 100%)">
                        <h6>Nápoje</h6>
                        <img src="https://static.wixstatic.com/media/9971db_55f2a83049824c70b708d6d3fc3b8810~mv2.png/v1/crop/x_106,y_0,w_625,h_537/fill/w_280,h_238,al_c,q_95,enc_auto/napoje.png"
                            alt="napoje image" class="img">
                    </div>
                </th>
                <th>
                    <div id="navFastFood" class="navItem"
                        style="background: linear-gradient(22deg, rgba(97, 255, 85, 1) 0%, rgba(226, 255, 101, 1) 100%);">
                        <h6>Rychlé jídlo</h6>
                        <img src="https://www.freepnglogos.com/uploads/food-png/high-resolution-food-clipart-collection-19.png"
                            alt="jídlo image" class="img">
                    </div>
                </th>
                <th>
                    <div id="navPotreby" class="navItem"
                        style="background: radial-gradient(circle, rgba(209, 23, 23, 1) 0%, rgba(209, 23, 23, 1) 100%);">
                        <h6>Školní potřeby</h6>
                        <img src="https://www.nonari.cz/user/categories/orig/jj.png" alt="Potreby image" class="img">
                    </div>
                </th>
            </tr>


        </thead>
    </table>

    <table>
        <thead>
            <th>
        
                    <!-- <div class="snacksItem milaItem">
                        <h1>Mila</h1>
                        <img src="https://www.plnalednice.cz/fotky91537/fotos/_vyr_1014_Mila_50g_SB_bocny_pohlad-1.png"
                            alt="Mila image" class="imgli">
                        <h6 class="cena cenasnacks1">15,00 Kč</h6>
                    </div> -->

                    <ul class="menu-items">
                        <% if (result && result.length > 0) { %>
                            <% result.forEach(function(products) { %>
                                <li>
                                    <div class="snacksItem <%= products.category %>Item">
                                        <h1><%= products.title %></h1>
                                        <img src="<%= products.image %>"
                                            alt=" <%= products.title %> image" class="imgli">
                                        <h6 class="cena cenasnacks1">15,00 Kč</h6>
                                    </div>
                                </li>
                            <% }); %>
                        <% } else { %>
                            <li>No item found</li>
                        <% } %>
                    </ul>

            </th>
        </thead>
    </table>
</body>

</html>